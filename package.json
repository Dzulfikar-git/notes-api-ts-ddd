{
  "name": "notes-api-ts-ddd",
  "version": "1.0.0",
  "description": "Notes API build with TS + Node while implementing DDD",
  "main": "index.js",
  "repository": "https://github.com/Dzulfikar-git/notes-api-ts-ddd.git",
  "author": "Dzulfikar Dzulfikar <kardzulfi1805@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "build": "rimraf ./dist && tsc",
    "migrate:create": "yarn run build && sequelize migration:create --migrations-path src/shared/infra/database/sequelize/migrations --config dist/shared/infra/database/sequelize/config.js --name ",
    "migrate:dev": "yarn run build && cross-env NODE_ENV=development sequelize db:migrate",
    "migrate:dev:undo": "yarn run build && cross-env NODE_ENV=development sequelize db:migrate:undo",
    "seed:create": "yarn run build && sequelize seed:create --seeders-path src/shared/infra/database/sequelize/seeders --config dist/shared/infra/database/sequelize/config.js --name  ",
    "seed:dev": "yarn run build && cross-env NODE_ENV=development sequelize db:seed:all",
    "seed:dev:undo": "yarn run build && cross-env NODE_ENV=development sequelize db:seed:undo"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "concurrently": "^8.0.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "prettier": "2.8.8",
    "sequelize-cli": "^6.6.0"
  },
  "dependencies": {
    "@types/dompurify": "^3.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.1",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dompurify": "^3.0.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.1.5",
    "install": "^0.13.0",
    "jest": "^29.5.0",
    "jsdom": "^21.1.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.1.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.2.4",
    "nodemon": "^2.0.22",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.31.0",
    "sequelize-typescript": "^2.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "uuidv4": "^6.2.13"
  }
}
